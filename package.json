{
  "name": "the-gazelle",
  "version": "1.0.0",
  "description": "Minimal starter kit for React, Falcor, and Universal rendering with ES6",
  "main": "server.js",
  "scripts": {
    "start": "node ./build/server.js",
    "build-production": "./node_modules/.bin/webpack -p --config ./webpack.production.config.js --bail",
    "build-beta": "./node_modules/.bin/webpack -p --config ./webpack.beta.config.js --bail",
    "build-emulate-ci": "CI=true CIRCLECI=true npm run build-production",
    "test:unit": "jest --testPathPattern='.*__tests__/(?!end-to-end/).*\\.test\\.js'",
    "test:e2e": "jest --testPathPattern='__tests__/end-to-end/.*\\.test\\.js' --runInBand",
    "lint": "eslint --max-warnings 0 '{src,__tests__}/**/*.{js,jsx}'",
    "precommit": "npm run lint && npm run test:unit"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "./src",
      "."
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thegazelle-ad/gazelle-front-end.git"
  },
  "keywords": [
    "react",
    "es6",
    "falcor",
    "universal"
  ],
  "author": "Lingliang Zhang (lingliangz@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thegazelle-ad/gazelle-front-end/issues"
  },
  "homepage": "https://github.com/thegazelle-ad/gazelle-front-end#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.4.5",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.2.1",
    "babel-plugin-lodash": "^3.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "nightmare": "^2.10.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.9.1",
    "sass-loader": "^4.0.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-node-externals": "^1.2.0"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.15.2",
    "child_process": "^1.0.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "express": "^4.13.4",
    "falcor": "^0.1.17",
    "falcor-express": "^0.1.2",
    "falcor-http-datasource": "^0.1.3",
    "falcor-router": "^0.4.0",
    "google-auth-library": "^0.9.8",
    "googleapis": "^12.4.0",
    "history": "^3.0.0",
    "imagemin": "^5.3.1",
    "imagemin-jpeg-recompress": "^5.1.0",
    "knex": "^0.11.10",
    "lodash": "^4.13.1",
    "material-ui": "^0.17.1",
    "moment": "^2.14.1",
    "mongodb": "^2.1.18",
    "multer": "^1.3.0",
    "mysql": "^2.11.1",
    "react": "^15.4.2",
    "react-addons-transition-group": "^15.3.0",
    "react-addons-update": "^15.3.1",
    "react-dom": "^15.4.2",
    "react-ga": "^2.1.2",
    "react-helmet": "^3.1.0",
    "react-router": "^2.4.1",
    "react-tap-event-plugin": "^2.0.1",
    "s3": "^4.4.0",
    "stable": "^0.1.5"
  }
}
