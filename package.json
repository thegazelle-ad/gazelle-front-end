{
  "name": "the-gazelle",
  "version": "1.0.0",
  "engines": {
    "node": "~9.3.0",
    "npm": "~5.6.0"
  },
  "description": "Minimal starter kit for React, Falcor, and Universal rendering with ES6",
  "main": "server.js",
  "scripts": {
    "start": "node ./build/server.js",
    "build": "webpack --config webpack/webpack.dev.config.js",
    "build-production": "webpack --config webpack/webpack.production.config.js --bail",
    "build-beta": "webpack --config webpack/webpack.beta.config.js --bail",
    "build-emulate-ci": "CI=true CIRCLECI=true npm run build-production",
    "test:unit": "jest --testPathPattern='.*__tests__/(?!end-to-end/).*\\.test\\.js'",
    "test:e2e": "jest --testPathPattern='__tests__/end-to-end/.*\\.test\\.js' --runInBand",
    "lint": "eslint --max-warnings 0 '{src,__tests__}/**/*.{js,jsx}'",
    "precommit": "TEMP_STASH_OUTPUT=$(git stash --keep-index); if [ \"$TEMP_STASH_OUTPUT\" != 'No local changes to save' ]; then lint-staged; PRECOMMIT_PASSED=$(echo $?); MUTE=$(git stash pop); exit $PRECOMMIT_PASSED; fi; exit 0;"
  },
  "lint-staged": {
    "*.js": "eslint --max-warnings 0",
    "*": "jest --bail --findRelatedTests"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "./src",
      "."
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thegazelle-ad/gazelle-front-end.git"
  },
  "keywords": [
    "react",
    "es6",
    "falcor",
    "universal"
  ],
  "author": "Lingliang Zhang (lingliangz@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thegazelle-ad/gazelle-front-end/issues"
  },
  "homepage": "https://github.com/thegazelle-ad/gazelle-front-end#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^5.0.0",
    "nightmare": "^2.10.0",
    "node-sass": "^4.7.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.9",
    "regenerator-runtime": "^0.11.1",
    "sass-loader": "^6.0.6",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "aws-sdk": "^2.169.0",
    "babel-cli": "^6.26.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.15.2",
    "child_process": "^1.0.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "express": "^4.13.4",
    "falcor": "^1.0.3",
    "falcor-express": "^0.1.4",
    "falcor-http-datasource": "^0.1.3",
    "falcor-router": "^0.8.1",
    "google-auth-library": "^0.9.8",
    "googleapis": "^12.4.0",
    "imagemin": "^5.3.1",
    "imagemin-jpeg-recompress": "^5.1.0",
    "knex": "^0.14.2",
    "lodash": "^4.13.1",
    "material-ui": "^0.17.1",
    "moment": "^2.14.1",
    "mongodb": "^2.1.18",
    "multer": "^1.3.0",
    "mysql": "^2.11.1",
    "react": "^15.4.2",
    "react-addons-transition-group": "^15.3.0",
    "react-addons-update": "^15.3.1",
    "react-dom": "^15.4.2",
    "react-ga": "^2.1.2",
    "react-helmet": "^3.1.0",
    "react-router": "^2.4.1",
    "react-tap-event-plugin": "^2.0.1",
    "s3": "^4.4.0",
    "source-map-support": "^0.5.0",
    "stable": "^0.1.5"
  }
}
